{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "es6"],
      "jsx": "react",
      "esModuleInterop": true,
      "sourceMap": true,
      "strict": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*", "src/types/*"]
      }
    },
    "include": ["src/**/*"]
  }